{% extends 'KmcAdminBundle::layout.html.twig' %}

{% block content %}
<!-- BEGIN row -->
			<div class="row">
				<!-- BEGIN col-6 -->
				<div class="col-md-12 col-sm-12">
					<!-- BEGIN widget -->
					<div class="widget widget-card widget-card-rowspan2 dynamic-xs inverse-mode">
						<!-- BEGIN widget-card-cover -->
						<div class="widget-card-cover">
							<div class="cover-bg"></div>
							<img src="/bundles/kmcadmin/assets/img/landing-cover.jpg" alt="" />
						</div>
						<!-- END widget-card-cover -->
						<!-- BEGIN widget-card-content -->
						<div class="widget-card-content">
							<a href="#" class="widget-title-link">Dismiss</a>
							<h4 class="widget-title"><b>GETTING STARTED</b></h4>
						</div>
						<!-- END widget-card-content -->
						<!-- BEGIN widget-card-content -->
						<div class="widget-card-content p-b-5">
							<div class="text-center">
								<h3 class="m-b-0">Welcome back, Admin!</h3>
								<p class="opacity-7">We've assembled some links to get you started:</p>
							</div>
							<!-- BEGIN row -->
							<div class="row">
								<!-- BEGIN col-6 -->
								<div class="col-xs-6">
									<!-- BEGIN widget -->
									<ul class="widget widget-list m-b-0 no-bg inverse-mode">
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_season') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-calendar bg-gradient-blue"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des saisons</h4>
													<div class="widget-desc hidden-xs">Administrer les saisons en cours du club</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_price') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-money bg-gradient-purple"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des tarifs</h4>
													<div class="widget-desc hidden-xs">Administrer les tarifs disponibles sur le formulaire d'inscription</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_subscriptionlist') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-clipboard bg-gradient-green"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des inscriptions</h4>
													<div class="widget-desc hidden-xs">Visualiser la liste des inscits depuis le formulaires d'inscriptions</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_information_list') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-bar-chart bg-gradient-green-aqua"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Administration & statistique formulaire</h4>
													<div class="widget-desc hidden-xs">Visualiser les réponses du formulaires d'inscription</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
									</ul>
									<!-- END widget -->
								</div>
								<!-- END col-6 -->
								<!-- BEGIN col-6 -->
								<div class="col-xs-6">
									<!-- BEGIN widget -->
									<ul class="widget widget-list m-b-0 no-bg inverse-mode">
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_member_list_card') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-id-badge bg-gradient-orange"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des membres</h4>
													<div class="widget-desc hidden-xs">Administrer les membres du club</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_subscriptionlist') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-agenda bg-gradient-red"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des stages</h4>
													<div class="widget-desc hidden-xs">Créer, modifier, suivre les inscriptions des stages</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
										<li>
											<!-- BEGIN widget-list-container -->
											<a href="{{ path('kmc_admin_clublist') }}" class="widget-list-container">
												<div class="widget-list-media icon p-l-0">
													<i class="ti-star bg-gradient-silver"></i>
												</div>
												<div class="widget-list-content">
													<h4 class="widget-title">Gestion des clubs</h4>
													<div class="widget-desc hidden-xs">Administrer les différentes sections de votre club</div>
												</div>
											</a>
											<!-- END widget-list-container -->
										</li>
									</ul>
									<!-- END widget -->
								</div>
								<!-- END col-6 -->
							</div>
							<!-- END row -->
						</div>
						<!-- END widget-card-content -->
					</div>
					<!-- END widget -->
				</div>
				<!-- END col-6 -->
			</div>
{% endblock %}